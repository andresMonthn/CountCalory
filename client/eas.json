{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "preview": {
      "env": {
        "VITE_API_URL": "https://countcalory.onrender.com/api"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "latest"
      }
    },
    "production": {
      "env": {
        "VITE_API_URL": "https://countcalory.onrender.com/api"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      }
    }
  }
}
